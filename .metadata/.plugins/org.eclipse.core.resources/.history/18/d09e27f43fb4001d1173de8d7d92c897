
public class TiposVariables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		byte b = 5;
		short s = 128;
		int i = 40000;
		
		long l = 2222222222222L;
		long longo = 2147483647L;
		int j = 2147483647;
		
		// el valor de j es incrementado
		j = j + 1;
		System.out.println(j);
	}

}
