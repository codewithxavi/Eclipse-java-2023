
public class BreakSentencies {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 3;
		while (true) {
			a++;
			if (a == 600000) {
				break;
			}
			
		}
		System.out.println("finalizado");
		
		ObtenerPrimo(2);
	}
	
	public static boolean ObtenerPrimo(int numero) {
		int n = numero;
		
		if(n<=1) return false;
		
		else if(n ==2) return true;
		
		else {
			for(int i=2; i<n; i++) {
				if(n%2 == 0) {
					return false;
				}
			}
			return true;
		}
	}

}
